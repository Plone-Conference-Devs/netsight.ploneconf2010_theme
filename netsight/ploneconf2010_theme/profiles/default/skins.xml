<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Plone Conf 2010 Theme">

 <object name="netsight_ploneconf2010_theme_custom_images"
    meta_type="Filesystem Directory View"
    directory="netsight.ploneconf2010_theme:skins/netsight_ploneconf2010_theme_custom_images"/>
 <object name="netsight_ploneconf2010_theme_custom_templates"
    meta_type="Filesystem Directory View"
    directory="netsight.ploneconf2010_theme:skins/netsight_ploneconf2010_theme_custom_templates"/>
 <object name="netsight_ploneconf2010_theme_styles"
    meta_type="Filesystem Directory View"
    directory="netsight.ploneconf2010_theme:skins/netsight_ploneconf2010_theme_styles"/>

 <skin-path name="Plone Conf 2010 Theme" based-on="Sunburst Theme">
  <layer name="netsight_ploneconf2010_theme_custom_images"
     insert-after="custom"/>
  <layer name="netsight_ploneconf2010_theme_custom_templates"
     insert-after="netsight_ploneconf2010_theme_custom_images"/>
  <layer name="netsight_ploneconf2010_theme_styles"
     insert-after="netsight_ploneconf2010_theme_custom_templates"/>
 </skin-path>

</object>
